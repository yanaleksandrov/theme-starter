{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "flade/example",
	"title": "Example",
	"icon": "universal-access-alt",
	"category": "flade-blocks",
	"attributes": {
		"content": {
			"type": "string",
			"source": "html",
			"selector": "p"
		}
	},
	"example": {
		"attributes": {
			"content": "Hello world"
		}
